  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "buildFlags": "-gcflags=\"all=-N\"",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/downloader",
